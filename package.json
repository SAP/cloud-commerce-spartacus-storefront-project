{
  "name": "storefrontapp",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --config ./.prettierrc --list-different \"projects/**/*{.ts,.js,.json,.scss}\"",
    "prettier-fix": "prettier --config ./.prettierrc --list-different \"projects/**/*{.ts,.js,.json,.scss}\" --write",
    "lint": "ng lint",
    "start": "ng serve --proxy-config mock-server/proxy.backend.js",
    "start:mock": "ng serve --proxy-config mock-server/proxy.mock.js",
    "start:qa": "ng serve -c=qa",
    "start:prod": "ng serve --prod",
    "start:pwa": "cd ./dist/storefrontapp/ && http-server -p 4200",
    "mockserver": "cd ./projects/backend/mockgenerator && npm run start",
    "build:core:lib": "ng build storefrontlib --prod",
    "build:qa": "ng build -c=qa",
    "build": "ng build --prod",
    "test": "ng test",
    "test:core:lib": "ng test storefrontlib --code-coverage",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --configuration=ci --protractor-config=projects/storefrontapp-e2e/protractor.headless.conf.js"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.0.1",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/flex-layout": "^6.0.0-beta.15",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/material": "^6.0.1",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/pwa": "^0.6.8",
    "@angular/router": "^6.0.0",
    "@angular/service-worker": "^6.0.0",
    "@ng-bootstrap/ng-bootstrap": "^2.0.0",
    "@ng-select/ng-select": "2.1.3",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "hamburgers": "^0.9.3",
    "hammerjs": "2.0.8",
    "ngrx-store-localstorage": "^5.0.1",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular-devkit/build-ng-packagr": "~0.6.1",
    "@angular/cli": "~6.0.1",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "faker": "^4.1.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.12.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "ng-packagr": "^3.0.0-rc.2",
    "ngrx-store-freeze": "^0.2.2",
    "node-sass": "^4.9.3",
    "prettier": "1.13.4",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
