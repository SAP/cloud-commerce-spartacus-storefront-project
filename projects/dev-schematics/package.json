{
  "name": "@spartacus/dev-schematics",
  "version": "0.0.1",
  "description": "Spartacus dev-schematics",
  "keywords": [
    "spartacus",
    "dev-schematics"
  ],
  "scripts": {
    "clean": "rm -rf src/**/*.js src/**/*.js.map src/**/*.d.ts",
    "build": "yarn clean && yarn copy:outlets:files && tsc -p tsconfig.json",
    "test": "yarn build && jasmine \"src/**/*_spec.js\"",
    "copy:outlets:files": "cd ./scripts && ./outlet-template-files.sh && cd .."
  },
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^9.1.0",
    "@angular-devkit/schematics": "^9.1.0",
    "@schematics/angular": "^9.1.0",
    "@spartacus/schematics": "^2.0.0-rc.4"
  },
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "@types/node": "^12.11.1",
    "jasmine": "^3.5.0",
    "typescript": "~3.8.3"
  },
  "ng-add": {
    "save": "devDependencies"
  }
}
