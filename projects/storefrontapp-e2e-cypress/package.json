  "scripts": {
    "cy:open": "cypress open",
    "cy:open:b2b": "cypress open --env BASE_SITE=powertools-spa,OCC_PREFIX_USER_ENDPOINT=orgUsers",
    "cy:open:epd-visualization": "cypress open --env BASE_SITE=powertools-epdvisualization-spa",
    "cy:run": "cypress run --browser chrome --config-file cypress.config.ts",
    "cy:run:b2b": "cypress run --browser chrome --config-file cypress.config.ts --env BASE_SITE=powertools-spa,OCC_PREFIX_USER_ENDPOINT=orgUsers",
    "cy:run:core": "cypress run --browser chrome --config-file cypress.config.ts --spec \"cypress/e2e/**/*.core-e2e.cy.ts\"",
    "cy:run:product-configurator-vc": "cypress run --browser chrome --config-file cypress.config.ts --spec \"cypress/e2e/**/*-configurator*.*e2e.cy*.ts\"",
    "cy:run:cpq": "cypress run --browser chrome --config-file cypress.config.ci.ts --config baseUrl=https://jsapps.cpce-teamtiger1-d2-public.model-t.cc.commerce.ondemand.com,defaultCommandTimeout=100000,requestTimeout=1000000,responseTimeout=1000000,pageLoadTimeout=1000000 --reporter junit --reporter-options mochaFile=results/test-results-[hash].xml --env API_URL=https://api.cpce-teamtiger1-d2-public.model-t.cc.commerce.ondemand.com --spec \"cypress/e2e/**/cpq-configuration.core-e2e.cy.ts\"",
    "cy:run:ci": "cypress run --browser chrome --config-file cypress.config.ci.ts --record --key $CYPRESS_KEY --tag \"b2c,all\" --parallel --ci-build-id $BUILD_NUMBER --group B2C --spec \"cypress/e2e/!(vendor|b2b|ssr)/**/*e2e.cy.ts\" --reporter-options mochaFile=results/e2e-test-b2c-[hash].xml",
    "cy:run:ci:core": "cypress run --browser chrome --config-file cypress.config.ci.ts --record --key $CYPRESS_KEY --tag \"b2c,core\" --parallel --ci-build-id $BUILD_NUMBER --group B2C --spec \"cypress/e2e/!(vendor|b2b|ssr)/**/*.core-e2e.cy.ts\"",
    "cy:run:ci:b2b": "cypress run --browser chrome --config-file cypress.config.ci.ts --env BASE_SITE=powertools-spa,OCC_PREFIX_USER_ENDPOINT=orgUsers --record --key $CYPRESS_KEY --tag \"b2b,all\" --parallel --ci-build-id $BUILD_NUMBER --group B2B --spec \"cypress/e2e/b2b/regression/**/*e2e.cy.ts\" --reporter junit --reporter-options mochaFile=results/e2e-test-b2b-[hash].xml",
    "cy:run:ci:core:b2b": "cypress run --browser chrome --config-file cypress.config.ci.ts --env BASE_SITE=powertools-spa,OCC_PREFIX_USER_ENDPOINT=orgUsers --record --key $CYPRESS_KEY --tag \"b2b,core\" --parallel --ci-build-id $BUILD_NUMBER --group B2B --spec \"cypress/e2e/b2b/regression/**/*.core-e2e.cy.ts\"",
    "cy:run:ci:ssr": "cypress run --browser chrome --config-file cypress.config.ci.ts --config baseUrl=http://localhost:4000 --record --key $CYPRESS_KEY --tag \"ssr,all\" --parallel --ci-build-id $BUILD_NUMBER --group SSR --spec \"cypress/e2e/ssr/pages.core-e2e.cy.ts\" --reporter junit --reporter-options mochaFile=results/e2e-test-ssr-[hash].xml",
    "cy:run:ci:ccv2": "cypress run --browser chrome --config-file cypress.config.ci.ts --reporter junit --reporter-options mochaFile=results/test-results-[hash].xml --env API_URL=https://api.cg79x9wuu9-eccommerc1-p1-public.model-t.myhybris.cloud --record --key $CYPRESS_KEY --tag \"b2c,ccv2\" --parallel --group CCV2-B2C --spec \"cypress/e2e/cx_ccv2/regression/b2c/**/*e2e.cy.ts\"",
    "cy:run:ci:ccv2-b2b": "cypress run --browser chrome --config-file cypress.config.ci.ts --reporter junit --reporter-options mochaFile=results/test-results-[hash].xml --env API_URL=https://api.cg79x9wuu9-eccommerc1-p1-public.model-t.myhybris.cloud,BASE_SITE=powertools-spa,OCC_PREFIX_USER_ENDPOINT=orgUsers --record --key $CYPRESS_KEY --tag \"b2b,ccv2\" --parallel --group CCV2-B2B --spec \"cypress/e2e/cx_ccv2/regression/b2b/**/*e2e.cy.ts\"",
    "cy:run:ci:cds": "cypress run --browser chrome --config-file cypress.config.ci.ts --record --key $CYPRESS_KEY --tag \"2011,b2c,all-cds\" --group CDS --spec \"cypress/e2e/vendor/cds/**/*.core-e2e.cy.ts\"",
    "cy:run:ci:cdc": "cypress run --browser chrome --config-file cypress.config.ci.ts --env API_URL=https://cdc-spa-2011-2102.demo.hybris.com --record --key $CYPRESS_KEY --tag \"2005,cdc\" --group CDC --spec \"cypress/e2e/vendor/cdc/**/*e2e.cy.ts\"",
    "cy:run:ci:digital-payments": "cypress run --browser chrome --config-file cypress.config.ci.ts --env API_URL=https://backoffice.cp96avkh5f-sapcxteam1-d5-public.model-t.cc.commerce.ondemand.com --record --key $CYPRESS_KEY --tag \"2105,digital-payments\" --group DIGITAL-PAYMENTS --spec \"cypress/e2e/vendor/digital-payments/*e2e.cy.ts\"",
    "cy:run:ci:epd-visualization": "cypress run --browser chrome --config-file cypress.config.ci.ts --env API_URL=https://api.cp96avkh5f-integrati1-d1-public.model-t.cc.commerce.ondemand.com,BASE_SITE=powertools-epdvisualization-spa --record --key $CYPRESS_KEY --tag \"2105,epd-visualization\" --group EPD_VISUALIZATION --ci-build-id $TRAVIS_BUILD_ID --spec \"cypress/e2e/vendor/epd-visualization/**/*e2e.cy.ts\""
  },
