{
  "$schema": "http://json-schema.org/schema",
  "$id": "ProductMultiDimensionalSchematics",
  "title": "Product Multi-Dimensional Schematics",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "projectName"
      }
    },
    "debug": {
      "description": "Display additional details during the running process.",
      "type": "boolean",
      "default": false
    },
    "lazy": {
      "type": "boolean",
      "description": "Lazy load the Product feature.",
      "default": true
    },
    "features": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "enum": ["Product-multi-dimensional-selector"],
        "type": "string"
      },
      "default": ["Product-multi-dimensional-selector"]
    }
  },
  "required": []
}
