  "schematics": "./schematics/collection.json",
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "peerDependencies": {
    "@angular/common": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/router": "^10.1.0",
    "@angular-devkit/schematics": "^10.1.0",
    "rxjs": "^6.6.0",
    "@spartacus/core": "3.0.0-next.2",
    "@spartacus/storefront": "3.0.0-next.2"
