{
  "name": "@spartacus/user",
  "version": "3.0.0",
  "description": "User feature library for Spartacus",
  "keywords": [
    "spartacus",
    "framework",
    "storefront",
    "user"
  ],
  "homepage": "https://github.com/SAP/spartacus",
  "repository": "https://github.com/SAP/spartacus/tree/develop/feature-libs/user",
  "license": "Apache-2.0",
  "scripts": {
    "build:schematics": "yarn clean:schematics && tsc -p ./tsconfig.schematics.json",
    "clean:schematics": "yarn prepare:schematics && rimraf \"schematics/**/*.js\" \"schematics/**/*.js.map\" \"schematics/**/*.d.ts\"",
    "prepare:schematics": "yarn install",
    "test:schematics": "yarn --cwd ../../projects/schematics/ run clean && yarn clean:schematics && jest --config ./jest.schematics.config.js"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^12.11.1",
    "@types/shelljs": "^0.8.7",
    "jest": "^26.6.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "ts-jest": "^26.4.3",
    "typescript": "~4.0.2"
  },
  "peerDependencies": {
    "@angular/common": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/forms": "^10.0.0",
    "@angular/router": "^10.1.0",
    "@spartacus/core": "3.1.0-next.0",
    "@spartacus/storefront": "3.1.0-next.0",
    "@spartacus/styles": "3.1.0-next.0",
    "chalk": "^4.1.0",
    "rxjs": "^6.6.0",
    "typescript": "~4.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "schematics": "./schematics/collection.json"
}
